{
  "name": "dinogram",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-transition-group": "^4.4.0",
    "autoprefixer": "^10.0.4",
    "chokidar-cli": "^2.1.0",
    "graphql": "^15.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "tailwindcss": "^2.0.1",
    "typescript": "~4.0.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "build:css": "npx tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "watch:css": "npx chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/styles/tailwind.output.css -c 'npm run build:css'",
    "start:dev": "npx npm-run-all build:css --parallel watch:css start:react",
    "start": "npm run build:css && react-scripts start",
    "start:react": "react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
